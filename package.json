{
  "name": "svql",
  "version": "0.0.28",
  "description": "FetchQL wrapper for Svelte 3",
  "main": "dist/svql.js",
  "svelte": "src/main.js",
  "module": "dist/svql.es.js",
  "unpkg": "dist/svql.min.js",
  "browser": "dist/svql.min.js",
  "files": [
    "src/**",
    "dist/*"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "serve": "sirv start docs --dev --quiet --single --port 8080",
    "lint": "eslint src e2e",
    "prepublishOnly": "NODE_ENV=production npm run build",
    "pretest": "npm run lint && NODE_ENV=test npm run build",
    "test": "npm run test:e2e -- ${BROWSER:-chrome:headless} -a 'npm run serve' e2e/cases",
    "test:e2e": "BASE_URL=http://localhost:8080 testcafe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pateketrueke/svql.git"
  },
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "keywords": [
    "svelte",
    "fetchql",
    "graphql",
    "svelte-graphql",
    "svelte3-graphql"
  ],
  "peerDependencies": {
    "svelte": "3.x"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-utils": ">=1.4.1",
    "minimist": ">=1.2.2",
    "rollup": "^2.8.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.3",
    "serialize-javascript": ">=2.1.1",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.18.2",
    "testcafe": "^1.5.0",
    "yrv": "0.0.30"
  },
  "dependencies": {
    "fetchql": "^3.0.0"
  }
}
