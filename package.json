{
  "name": "svql",
  "version": "0.0.5",
  "description": "FetchQL wrapper for Svelte 3",
  "main": "dist/svql.js",
  "svelte": "src/main.js",
  "module": "dist/svql.es.js",
  "browser": "dist/svql.min.js",
  "files": [
    "src/**",
    "dist/*"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "NODE_ENV=production rollup -c",
    "serve": "live-server docs --quiet --no-browser --entry-file=index.html",
    "lint": "eslint src tests",
    "prepublishOnly": "npm run test && npm run build",
    "test:e2e": "BASE_URL=http://localhost:8080 testcafe",
    "test:live": "BASE_URL=http://localhost:8080 testcafe-live",
    "test:e2e:ci": "npm run test:e2e -- --ports 9505,9506 remote:1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pateketrueke/svql.git"
  },
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "keywords": [
    "svelte",
    "client",
    "fetchql",
    "graphql"
  ],
  "peerDependencies": {
    "svelte": "3.x"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-utils": ">=1.4.1",
    "live-server": "^1.2.1",
    "lodash": ">=4.17.13",
    "rollup": "^1.14.5",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.0.0",
    "svelte": "^3.4.4",
    "svero": "^1.0.0",
    "ws": ">=3.3.1",
    "express": ">=3.11.0",
    "useragent": ">=2.1.13",
    "tough-cookie": ">=2.3.3",
    "ejs": ">=2.5.5",
    "qs": ">=1.0.0",
    "uglify-js": ">=2.6.0",
    "send": ">=0.11.1",
    "moment": ">=2.19.3",
    "testcafe": "^15.1.317922"
  },
  "dependencies": {
    "fetchql": "^3.0.0"
  }
}
