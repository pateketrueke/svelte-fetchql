{
  "name": "svelte-fetchql",
  "version": "0.0.0",
  "description": "FetchQL wrapper for Svelte 3",
  "main": "dist/svelte-fetchql.js",
  "module": "dist/svelte-fetchql.es.js",
  "browser": "dist/svelte-fetchql.umd.js",
  "files": [
    "dist/*.*"
  ],
  "scripts": {
    "build:browser": "bili src/main.js --format umd-min --module-name svelte-fetchql --minimal --file-name svelte-fetchql.umd.js",
    "build:node": "bili src/main.js --minimal --format cjs --file-name svelte-fetchql.js",
    "build:esm": "bili src/main.js --minimal --format es --file-name svelte-fetchql.es.js",
    "build": "npm run build:browser && npm run build:node && npm run build:esm",
    "dev": "bili --format iife --minimal tests/e2e/*.test.js -d docs -w",
    "serve": "live-server docs --quiet --no-browser --entry-file=index.html --proxy=/api:http://localhost:8081/api",
    "watch": "npm test -- -w",
    "lint": "eslint src tests",
    "test": "npm run test:e2e:ci",
    "pretest": "npm run lint",
    "prepublish": "npm run build",
    "test:e2e": "BASE_URL=http://localhost:8080 testcafe",
    "test:live": "BASE_URL=http://localhost:8080 testcafe-live",
    "test:e2e:ci": "npm run test:e2e -- --ports 9505,9506 remote:1"
  },
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "bili": "^4.4.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "fetchql": "^3.0.0",
    "live-server": "^1.2.1",
    "rollup-plugin-svelte": "^5.0.3",
    "svelte": "^3.4.4",
    "svero": "^0.5.0",
    "testcafe": "^1.1.0"
  }
}
